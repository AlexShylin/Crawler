<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="cache" class="com.gd.ashylin.cache.accessor.CacheImpl">
        <property name="cacheRequestValidator" ref="cacheRequestValidator"/>
        <property name="parser" ref="cacheParser"/>
        <property name="ip" value="localhost"/>
        <property name="port" value="9750"/>
    </bean>

    <bean id="cacheRequestValidator" class="com.gd.ashylin.cache.valid.CacheRequestValidatorImpl"/>

    <bean id="cacheParser" class="com.gd.ashylin.cache.accessor.parse.JsonParser">
        <constructor-arg name="valueClass" value="com.gd.ashylin.crawler.dto.CacheDto"/>
    </bean>
</beans>